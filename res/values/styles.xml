<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style
    name="YtdTheme"
    parent="android:Theme.Light.NoTitleBar">
    <item
      name="android:windowBackground">@color/white</item>
    <item
      name="android:colorBackground">@color/white</item>
    <item
      name="android:panelColorBackground">@color/white</item>
  </style>
    <color name="WHITE">#ffffff</color>
    <color name="titlebackgroundcolor">#00abd7</color>
    <color name="titlecolor">#666666</color>
     <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
<style name="SettingCodeFontBold" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textStyle">bold</item>
</style> 
<style name="SettingCodeFont" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:typeface">monospace</item>
        
</style>
</resources>